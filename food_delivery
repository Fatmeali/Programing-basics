chicken_menus=int(input())
fish_menus=int(input())
veg_menus=int(input())
price_delivery=2.50

price_chicken_menus=chicken_menus*10.35
price_fish_menus=fish_menus*12.40
price_veg_menus=veg_menus*8.15
total_price_menus=price_chicken_menus+price_fish_menus+price_veg_menus
price_dessert=total_price_menus*0.2
total_price_order=total_price_menus+price_dessert+price_delivery

print(total_price_order)
